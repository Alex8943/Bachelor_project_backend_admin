{
  "compilerOptions": {
  "target": "ES6",
  "module": "Commonjs",
  "outDir": "./dist",
  "rootDir": "./",
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true,
  "skipLibCheck": true,
  "baseUrl": "./src",
  "paths": {
      "@config/*": ["config/*"]
    }
},
"include": [
  "src/**/*.ts",
  "config.ts"
]

}