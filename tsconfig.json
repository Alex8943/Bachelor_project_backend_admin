{
  "compilerOptions": {
  "target": "ES6",
  "module": "Commonjs",
  "outDir": "./dist",
  "rootDir": "./",
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true,
  "skipLibCheck": true,
  "baseUrl": "./src",
  "paths": {
      "@config/*": ["config/*"]
    },
  "moduleResolution": "node",
  "resolveJsonModule": true,
},
"include": [
  "src/**/*.ts",
  "config.ts"
]

}